<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<link rel="stylesheet" href="/stylesheets/style.css" />
	</head>
	<body>
		<h1><%= title %></h1>
		<ul>
			<% category_list.forEach(function(category) { %>
			<li>
				<h2>
					<a href="<%=category.url%>">
						<%= category.name %>
					</a>
				</h2>
				<p><%= category.description %></p>
			</li>
			<% }); %>
		</ul>
	</body>
</html>
